{
  "name": "pdfkit",
  "description": "A PDF generation library for Node.js",
  "keywords": [
    "pdf",
    "pdf writer",
    "pdf generator",
    "graphics",
    "document",
    "vector"
  ],
  "version": "0.1.9",
  "homepage": "http://pdfkit.org/",
  "author": {
    "name": "Devon Govett",
    "email": "devongovett@gmail.com",
    "url": "http://badassjs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/devongovett/pdfkit.git"
  },
  "bugs": {
    "name": "http://github.com/devongovett/pdfkit/issues"
  },
  "directories": {
    "lib": "./lib",
    "example": "./demo"
  },
  "devDependencies": {
    "coffee-script": ">=1.0.1"
  },
  "dependencies": {
    "png-js": ">=0.1.0"
  },
  "scripts": {
    "prepublish": "coffee -o js -c lib/ && cp -r lib/font/data js/font/data",
    "postpublish": "rm -rf ./js"
  },
  "main": "./js/document",
  "engine": [
    "node >= v0.6.0"
  ],
  "_id": "pdfkit@0.1.9",
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  },
  "_engineSupported": true,
  "_npmVersion": "1.1.4",
  "_nodeVersion": "v0.6.12",
  "_defaultsLoaded": true,
  "_from": "pdfkit@*"
}
